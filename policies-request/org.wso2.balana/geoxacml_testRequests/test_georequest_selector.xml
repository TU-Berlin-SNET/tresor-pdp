<?xml version="1.0" encoding="UTF-8"?>
<Request
 xmlns="urn:oasis:names:tc:xacml:2.0:context:schema:os"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="urn:oasis:names:tc:xacml:2.0:context:schema:os
http://docs.oasis-open.org/xacml/access_control-xacml-2.0-context-schema-os.xsd"
xmlns:gml="http://www.opengis.net/gml">

        
    <Subject/>
    
    
    <Resource>

        <ResourceContent>
            <eig:bbox xmlns:eig="http://www.environmatics.net">
                <gml:Box srsName="http://www.opengis.net/gml/srs/epsg.xml#4326">
                    <gml:coord>
                        <gml:X>-100.0</gml:X>
                        <gml:Y>0.0</gml:Y>
                    </gml:coord>
                    <gml:coord>
                        <gml:X>100.0</gml:X>
                        <gml:Y>100.0</gml:Y>
                    </gml:coord>
                </gml:Box>
            </eig:bbox>
        </ResourceContent>

        <Attribute AttributeId="urn:oasis:names:tc:xacml:1.0:resource:resource-id"
                   DataType="http://www.w3.org/2001/XMLSchema#string">
            <AttributeValue>SampleServer</AttributeValue>
        </Attribute>
    </Resource>
    
    
    <Action>
        <Attribute AttributeId="ServerAction"
               DataType="http://www.w3.org/2001/XMLSchema#string">
            <AttributeValue>login</AttributeValue>
        </Attribute>
        <Attribute AttributeId="ServerAction" DataType="http://www.w3.org/2001/XMLSchema#string">
            <AttributeValue>
                login
            </AttributeValue>
            
            
        </Attribute>
        
    </Action>

    
    <Environment/>
</Request>


